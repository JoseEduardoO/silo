<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.projetosilo.AluguelController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Menu">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#voltarMenu" text="Voltar" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Ajuda">
          <items>
            <MenuItem mnemonicParsing="false" text="Ajuda" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="148.0" layoutY="38.0" style="&#10;" text="Cadastro de Aluguel" textAlignment="CENTER" textFill="#4ad413" wrapText="false">
          <font>
            <Font name="Comic Sans MS" size="37.0" />
          </font>
        </Label>
            <TextField fx:id="campoNome" layoutX="207.0" layoutY="157.0">
               <font>
                  <Font name="SansSerif Regular" size="12.0" />
               </font>
            </TextField>
            <TextField fx:id="campoQuantidade" layoutX="207.0" layoutY="203.0">
               <font>
                  <Font name="SansSerif Regular" size="12.0" />
               </font>
            </TextField>
            <Button layoutX="343.0" layoutY="296.0" mnemonicParsing="false" onAction="#cadastrarAluguel" text="Salvar" />
            <Label layoutX="14.0" layoutY="158.0" text="Nome do Produtor">
               <font>
                  <Font name="SansSerif Regular" size="16.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="114.0" text="Data de Entrada">
               <font>
                  <Font name="Arial" size="16.0" />
               </font>
            </Label>
            <Label layoutX="38.0" layoutY="204.0" text="Quantidade">
               <font>
                  <Font name="Arial" size="16.0" />
               </font>
            </Label>
            <DatePicker fx:id="campoData" layoutX="207.0" layoutY="111.0" prefHeight="26.0" prefWidth="150.0" />
            <Label fx:id="campoDeStatus" layoutX="45.0" layoutY="299.0" prefHeight="17.0" prefWidth="102.0" />
            <Label layoutX="12.0" layoutY="246.0" text="Origem do Produto">
               <font>
                  <Font name="Arial" size="16.0" />
               </font></Label>
            <TextField fx:id="campoOrigem" layoutX="208.0" layoutY="243.0" />
            <Button layoutX="182.0" layoutY="295.0" mnemonicParsing="false" onAction="#limparCampos" text="Limpar" />
      </children>
    </AnchorPane>
  </children>
</VBox>
